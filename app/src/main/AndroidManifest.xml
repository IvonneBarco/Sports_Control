<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practicavolley.ennovic.sportscontrol">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono_sportscontrol"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono_sportscontrol"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".Deportes" />
        <activity android:name=".Ligas" />
        <activity android:name=".Entrenadores" />
        <activity android:name=".Atletas" />
        <activity android:name=".InicioEntrenamiento" />
        <activity android:name=".EntrenoProgramado" />
        <activity android:name=".PlanSemanal" />
        <activity android:name=".SportsActivity" />
        <activity android:name=".AtletasEntrenador"/>
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>