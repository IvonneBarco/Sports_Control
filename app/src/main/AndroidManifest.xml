<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practicavolley.ennovic.sportscontrol">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icono_sportscontrol"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icono_sportscontrol"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Actividades.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Actividades.HomeActivity" />
        <activity android:name=".Actividades.ProfileActivity" />
        <activity android:name=".Actividades.Deportes" />
        <activity android:name=".Actividades.Ligas" />
        <activity android:name=".Actividades.Entrenadores" />
        <activity android:name=".Actividades.Atletas" />
        <activity android:name=".Actividades.InicioEntrenamiento" />
        <activity android:name=".Actividades.EntrenoProgramado" />
        <activity android:name=".Actividades.PlanSemanal" />
        <activity android:name=".Actividades.AtletasEntrenador" />
        <activity
            android:name=".Actividades.OpcionesActivity"
            android:logo="@drawable/logo_sportscontrol"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>