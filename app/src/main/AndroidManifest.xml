<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.practicavolley.ennovic.sportscontrol">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_sportscontrol"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_sportscontrol"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".AthletesActivity"/>
        <activity android:name=".Deportes" />
        <activity android:name=".Ligas" />
        <activity android:name=".Entrenadores" />
        <activity android:name=".Atletas"/>
        <activity android:name=".Registrar2"/>
        <activity android:name=".Registrar"></activity>
    </application>

</manifest>